{
        "port": 8000,
        "engine": "memory",
        "prometheus": false,
        "allowed_origins": [
                "*"
        ],
        "proxy_http_headers": [
                "Cookie"
        ],
        "namespaces": [
                {
                        "force_recovery": true,
                        "name": "beers",
                        "presence": true,
                        "join_leave": false,
                        "history_size": 10,
                        "history_ttl": "30s",
                        "allow_publish_for_subscriber": false,
                        "allow_user_limited_channels": true
                },
                {
                        "force_recovery": true,
                        "name": "rooms",
                        "presence": true,
                        "join_leave": false,
                        "history_size": 10,
                        "history_ttl": "30s",
                        "allow_publish_for_subscriber": false,
                        "allow_user_limited_channels": true
                },
                {
                        "name": "user",
                        "allow_user_limited_channels": true
                }
        ],
        "token_hmac_secret_key": "secret_hmac_key",
        "api_key": "secret_api_key",
        "admin_password": "ADMIN_PASSWORD",
        "admin_secret": "ADMIN_SECRET",
        "admin": true,
        "log_level": "debug",
        "debug": true
}
